{
  "name": "doem",
  "version": "0.2.9",
  "description": "A functional DOM traversal and manipulation library for modern browsers",
  "license": "MIT",
  "repository": "kasperisager/doem",
  "author": "Kasper Kronborg Isager <kasperisager@gmail.com>",
  "scripts": {
    "test": "xo && browserify -t babelify test/*.js | tape-run",
    "prepublish": "babel lib -d dist"
  },
  "main": "dist/index.js",
  "jsnext:main": "lib/index.js",
  "files": [
    "lib",
    "dist"
  ],
  "keywords": [
    "functional",
    "dom",
    "traversal",
    "manipulation",
    "es6",
    "modern",
    "browser"
  ],
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "tape": "^4.5.1",
    "tape-run": "^2.1.2",
    "xo": "^0.12.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "hljs": {
    "highlightAuto": true,
    "languages": [
      "js",
      "html",
      "css"
    ]
  },
  "xo": {
    "space": true,
    "exnext": true,
    "envs": [
      "browser"
    ]
  }
}
